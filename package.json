{
    "name": "cookhound",
    "version": "0.1.0",
    "type": "module",
    "private": true,
    "scripts": {
        "dev": "next dev --turbopack",
        "build": "next build && npm run copy-static && npm run setup-typesense",
        "copy-static": "node scripts/copy-standalone.js",
        "start": "node .next/standalone/server.js",
        "dev-worker": "tsx src/server/queues/worker.ts",
        "prepare": "husky",
        "test": "vitest run",
        "lint": "prettier --write . && eslint . --fix",
        "migrate": "npx prisma migrate dev --name init",
        "generate": "npx prisma generate --sql",
        "setup-typesense": "node scripts/setup-typesense-keys.js",
        "seed": "npx prisma db seed"
    },
    "dependencies": {
        "@babel/runtime": "^7.28.4",
        "@prisma/client": "^6.16.1",
        "@tanstack/react-query": "^5.87.4",
        "bcrypt": "^6.0.0",
        "bullmq": "^5.58.5",
        "class-transformer": "^0.5.1",
        "cookie": "^1.0.2",
        "dotenv": "^17.2.2",
        "framer-motion": "^12.23.12",
        "ioredis": "^5.7.0",
        "next": "15.5.3",
        "next-themes": "^0.4.6",
        "openai": "^5.20.1",
        "react": "^19.1.1",
        "react-dom": "^19.1.1",
        "react-image-crop": "^11.0.10",
        "reflect-metadata": "^0.2.2",
        "typesense": "^2.1.0",
        "uuid": "^13.0.0",
        "winston": "^3.17.0",
        "winston-daily-rotate-file": "^5.0.0",
        "zod": "^3.25.67",
        "zustand": "^5.0.8"
    },
    "devDependencies": {
        "@svgr/webpack": "^8.1.0",
        "@types/bcrypt": "^6.0.0",
        "@types/cookie": "^1.0.0",
        "@types/jsonwebtoken": "^9",
        "@types/node": "^22.6.1",
        "@types/react": "^19.1.12",
        "@types/react-dom": "^19.1.9",
        "@types/uuid": "^10",
        "@typescript-eslint/eslint-plugin": "^7.7.1",
        "@typescript-eslint/parser": "^7.7.1",
        "autoprefixer": "^10.4.21",
        "eslint": "^8",
        "eslint-config-next": "14.2.13",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-react": "^7.36.1",
        "eslint-plugin-storybook": "^0.8.0",
        "husky": "^9.1.7",
        "lint-staged": "^15.2.10",
        "postcss": "^8.5.6",
        "prettier": "^3.6.2",
        "prettier-plugin-tailwindcss": "^0.6.13",
        "prisma": "^6.16.1",
        "tailwindcss": "^3.4.17",
        "tsx": "^4.20.5",
        "typescript": "5.9.2",
        "vite-tsconfig-paths": "^5.1.4",
        "vitest": "^2.1.1"
    },
    "prisma": {
        "schema": "src/server/db/schema.prisma",
        "seed": "tsx prisma/seed.ts"
    },
    "packageManager": "yarn@4.1.0"
}
